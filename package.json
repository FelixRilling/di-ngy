{
    "name": "di-ngy",
    "version": "8.2.0-1",
    "description": "A small discord.js + cli-ngy boilerplate",
    "main": "./dist/dingy.common.js",
    "module": "./dist/esm/src/main.js",
    "types": "./dist/esm/src/main.d.ts",
    "rollupOptions": {
        "moduleName": "dingy",
        "outputName": "dingy",
        "external": [
            "path",
            "fs-extra",
            "discord.js",
            "yamljs",
            "lightdash",
            "logby",
            "chevronjs",
            "cli-ngy"
        ],
        "globals": {
            "lightdash": "l_",
            "logby": "logby",
            "chevronjs": "Chevron",
            "cli-ngy": "clingy",
            "yamljs": "YAML"
        }
    },
    "scripts": {
        "start": "npm run dist",
        "dist": "npm-run-all clean lint build test docs",
        "clean": "rimraf ./dist ./spec/dist ./docs",
        "build": "npm-run-all build:tsc build:bundle",
        "build:tsc": "tsc --build ./tsconfig.json",
        "build:bundle": "rollup -c rollup.config.js",
        "watch": "tsc --build ./tsconfig.json -w",
        "lint": "eslint ./src/ ./spec/src/ --ext .ts",
        "fix": "eslint ./src/ ./spec/src/ --ext .ts --fix",
        "test": "jasmine",
        "docs": "jsdoc -c jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/di-ngy.git"
    },
    "keywords": [
        "bot",
        "boilerplate",
        "discord"
    ],
    "author": "FelixRilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/di-ngy/issues"
    },
    "homepage": "https://github.com/FelixRilling/di-ngy#readme",
    "dependencies": {
        "@types/fs-extra": "^8.0.1",
        "@types/lodash": "^4.14.144",
        "@types/node": "^12.12.3",
        "@types/yamljs": "^0.2.30",
        "chevronjs": "^11.1.0",
        "cli-ngy": "^10.3.0",
        "discord.js": "^11.5.0",
        "fs-extra": "^8.0.1",
        "lightdash": "^10.3.0",
        "lodash": "^4.17.15",
        "logby": "^3.1.1",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/jasmine": "^3.4.4",
        "@typescript-eslint/eslint-plugin": "^2.6.0",
        "@typescript-eslint/parser": "^2.6.0",
        "docdash": "^1.1.1",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.5.0",
        "eslint-plugin-prettier": "^3.1.1",
        "jasmine": "^3.5.0",
        "jsdoc": "^3.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.18.2",
        "rimraf": "^3.0.0",
        "rollup": "^1.26.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "typescript": "^3.6.4"
    }
}
