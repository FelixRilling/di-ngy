{
    "name": "di-ngy",
    "version": "4.0.12",
    "description": "A small discord.js + cli-ngy boilerplate",
    "main": "dist/dingy.common.js",
    "module": "dist/dingy.esm.js",
    "types": "types/main.d.ts",
    "settings": {
        "input": "main.ts",
        "namespace": {
            "module": "dingy",
            "file": "dingy"
        },
        "external": [
            "lightdash",
            "cli-ngy",
            "flat-cache",
            "log",
            "make-fetch-happen"
        ]
    },
    "scripts": {
        "start": "node index.js",
        "mock": "node test/",
        "build": "rollup -c ./tasks/rollup.config.js",
        "build:iife": "rollup -c ./tasks/rollup.config.iife.js",
        "build:iife-min": "rollup -c ./tasks/rollup.config.iife-min.js",
        "watch": "rollup -c ./tasks/rollup.config.js --watch",
        "dist": "npm-run-all --parallel build build:iife build:iife-min",
        "lint": "eslint ./src/",
        "test": "jasmine",
        "docs": "jsdoc -c jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/di-ngy.git"
    },
    "keywords": ["bot", "boilerplate", "discord"],
    "author": "FelixRilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/di-ngy/issues"
    },
    "homepage": "https://github.com/FelixRilling/di-ngy#readme",
    "dependencies": {
        "cli-ngy": "^8.0.0",
        "discord.js": "11.3.0",
        "flat-cache": "^1.3.0",
        "lightdash": "^4.0.0",
        "log": "^1.4.0",
        "make-fetch-happen": "^2.6.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.38",
        "@babel/plugin-external-helpers": "^7.0.0-beta.38",
        "@babel/preset-env": "^7.0.0-beta.38",
        "docdash": "git+https://github.com/clenemt/docdash.git",
        "eslint": "^4.15.0",
        "jasmine": "^2.8.0",
        "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
        "npm-run-all": "^4.1.2",
        "rollup": "^0.54.1",
        "rollup-plugin-babel": "^4.0.0-beta.0",
        "rollup-plugin-json": "^2.3.0",
        "rollup-plugin-node-resolve": "^3.0.2",
        "rollup-plugin-typescript2": "^0.10.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "typescript": "^2.6.2",
        "typescript-eslint-parser": "^12.0.0",
        "uglify-es": "^3.3.7"
    },
    "engines": {
        "node": ">=8.0.0"
    }
}
