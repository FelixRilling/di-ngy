{
    "name": "di-ngy",
    "version": "5.1.4",
    "description": "A small discord.js + cli-ngy boilerplate",
    "main": "dist/dingy.common.js",
    "module": "dist/dingy.esm.js",
    "types": "types/main.d.ts",
    "settings": {
        "input": "main.ts",
        "namespace": {
            "module": "dingy",
            "file": "dingy"
        },
        "external": [
            "discord.js",
            "cli-ngy",
            "lightdash",
            "make-fetch-happen",
            "winston",
            "flat-cache"
        ]
    },
    "scripts": {
        "start": "npm-run-all lint build test docs",
        "build": "rollup -c ./tasks/rollup.config.js",
        "watch": "rollup -c ./tasks/rollup.config.js --watch",
        "lint": "npm-run-all --parallel lint:ts lint:es",
        "lint:ts": "tslint ./src/**/*.ts",
        "lint:es": "eslint ./spec/**/*.js",
        "fix": "npm-run-all fix:ts fix:es fix:prettier",
        "fix:ts": "tslint ./src/**/*.ts --fix",
        "fix:es": "eslint ./spec/**/*.js --fix",
        "fix:prettier": "prettier ./src/**/* ./spec/**/* --write",
        "test": "jasmine",
        "docs": "jsdoc -c jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/di-ngy.git"
    },
    "keywords": ["bot", "boilerplate", "discord"],
    "author": "FelixRilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/di-ngy/issues"
    },
    "homepage": "https://github.com/FelixRilling/di-ngy#readme",
    "dependencies": {
        "@types/node": "^9.4.0",
        "cli-ngy": "^8.0.6",
        "discord.js": "^11.3.0",
        "flat-cache": "^1.3.0",
        "lightdash": "^4.1.5",
        "make-fetch-happen": "^2.6.0",
        "winston": "^3.0.0-rc1"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.39",
        "@babel/plugin-external-helpers": "^7.0.0-beta.39",
        "@babel/preset-env": "^7.0.0-beta.39",
        "docdash": "git+https://github.com/clenemt/docdash.git",
        "eslint": "^4.17.0",
        "jasmine": "^2.9.0",
        "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
        "npm-run-all": "^4.1.2",
        "prettier": "^1.10.2",
        "rollup": "^0.55.3",
        "rollup-plugin-babel": "^4.0.0-beta.1",
        "rollup-plugin-json": "^2.3.0",
        "rollup-plugin-node-resolve": "^3.0.2",
        "rollup-plugin-typescript2": "^0.11.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "tslint": "^5.9.1",
        "typescript": "^2.7.1",
        "uglify-es": "^3.3.9"
    },
    "engines": {
        "node": ">=8.0.0"
    }
}
